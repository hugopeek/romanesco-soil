{
    "base": "assets/semantic/",
    "paths": {
        "source": {
            "config": "src/theme.config",
            "definitions": "src/definitions/",
            "site": "src/site/",
            "themes": "src/themes/"
        },
        "output": {
            "packaged": "dist/",
            "uncompressed": "dist/components/",
            "compressed": "dist/components/",
            "themes": "dist/themes/"
        },
        "clean": "dist/"
    },
    "permission": false,
    "autoInstall": true,
    "rtl": false,
    "components": [
        "reset",
        "site",
        "button",
        "container",
        "divider",
        "header",
        "icon",
        "image",
        "input",
        "label",
        "list",
        "segment",
        "text",
        "breadcrumb",
        "grid",
        "menu",
        "message",
        "card",
        "item",
        "statistic",
        "accordion",
        "sidebar",
        "sticky",
        "transition",
        "visibility"
    ],
    "individuals": [
        "form",
        "calendar",
        "checkbox",
        "dropdown",
        "embed",
        "popup",
        "rating",
        "tab",
        "table",
        "dimmer",
        "modal",
        "loader",
        "search",
        "step",
        "toast",
        "feed",
        "flag"
    ],
    "version": "2.9.3"
}